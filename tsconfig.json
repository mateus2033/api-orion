{
    "compilerOptions": {
        "outDir": "./dist",
        "module": "commonjs",
        "target": "ES2020",
        "esModuleInterop": true,
        "allowJs": true,
        "noImplicitReturns":false,
        "strictNullChecks": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "baseUrl": "src",
        "paths": {
            "@models/*": ["app/domain/model/*"],
            "@models": ["app/domain/model"] 
        }
    }
}